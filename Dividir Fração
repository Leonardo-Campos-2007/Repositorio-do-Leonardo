#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <math.h>

typedef struct
{

    float Numerador; 
    float Denominador;

} Fracao;

Fracao criarFracao(int N, int D) 
{

    Fracao F;
    F.Numerador = N;
    F.Denominador = D;

    return F;

}


int main()
{

    Fracao F1 = criarFracao(7, 2);
    printf("O valoir de F1 Ã© %.2f\n", (float)F1.Numerador/F1.Denominador);

}
